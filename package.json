    "@types/node": "^17.0.24",
    "ajv": "^8.11.0",
    "codelyzer": "^6.0.2",
    "cp-cli": "^2.0.0",
    "jasmine-core": "^4.1.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "^6.3.18",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^14.2.1",
    "protractor": "^7.0.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "tslint": "^6.1.3",
    "typescript": "^4.6.3"
